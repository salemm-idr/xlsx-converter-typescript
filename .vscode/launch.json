      // "args": ["${relativeFile}"],
      // "runtimeArgs": ["-r", "ts-node/register"],
      "outFiles": [
        "${workspaceRoot}/src/uploads/",
        "${workspaceRoot}/src/outputs/",
        "${workspaceRoot}/src/tiras",
        "${workspaceRoot}/src/constructedFile"
      ],
      // "program": "${workspaceRoot}/src/app.ts",
      "cwd": "${workspaceRoot}",
      "restart": true,
      // "console": "integratedTerminal",
      "internalConsoleOptions": "openOnSessionStart",
      "skipFiles": ["<node_internals>/**"],
      "sourceMaps": true,
      "protocol": "inspector"
    }
  ]
